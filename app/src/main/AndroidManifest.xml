            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT " />
            </intent-filter>
        </activity>
        <activity android:name=".create" />
        <activity android:name=".join" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>